<div id="root">
  <div class="dashboard-page">
    {{>header}}
    <main class="main" id="dashboard-main">
      <ul class="dashboard-page-navbar" id="dashboard-navbar">
        {{#each navItems}}
        {{>nav-item data=this}}
        {{/each}}
      </ul>
      <div class="main-content" id="main-content">
        <div class='dashboard-account-info'>
          <h3 class='info-header'>THÔNG TIN PREMIUM</h3>
          <div class='info-wrapper'>
            {{#if (isPremium data.premiumTime)}}
            {{>info-item label="Hạn Premium" data=data.premiumTime }}
            {{else}}
            {{>info-item label="Chưa đăng ký Premium" data=""}}
            {{/if}}
            <a href="/users/extend-premium">
              <div class='info-button-container'>
                {{>roundButton type="button" className="strawberry-backgroundcolor" id='extend-premium-btn' text='Gia
                hạn premium'}}
              </div>
            </a>
          </div>
        </div>
      </div>
    </main>
    {{!-- {{>footer}} --}}
  </div>
</div>